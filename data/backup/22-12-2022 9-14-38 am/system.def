yes.active.text = "YES"
yes.active.font = 1,3,0
yes.active.offset = -80, 60
no.text = "NO"
no.font = 1,0,0, 128,128,128
no.offset = 80, 60
no.active.text = "NO"
no.active.font = 1,3,0
no.active.offset = 80, 60

